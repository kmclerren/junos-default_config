#jinja2: lstrip_blocks: True
chassis {
    cluster {
        control-link-recovery;
        reth-count 2;
        heartbeat-interval 1000;
        heartbeat-threshold 3;
        /* this is the routing engine redundancy group, do not put interfaces in here */
        redundancy-group 0 {
            node 0 priority 100;
            node 1 priority 1;
        }
        /* interfaces go here */
        redundancy-group 1 {
            node 0 priority 100;
            node 1 priority 1;
            hold-down-interval 600;
            interface-monitor {
                xe-0/0/0 weight 255;
                xe-0/0/4 weight 255;
                xe-0/0/7 weight 255;
                xe-0/0/3 weight 255;
                xe-7/0/0 weight 255;
                xe-7/0/4 weight 255;
                xe-7/0/7 weight 255;
                xe-7/0/3 weight 255;
            }
        }
    }
}